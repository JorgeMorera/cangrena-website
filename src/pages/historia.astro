---
import Layout from "../layouts/Layout.astro"
import { Image } from 'astro:assets';
import cuerpo from "../assets/cuerpotiza.png";
---

<Layout title="Historia">

    <div class="flex justify-center items-center">
        <div class="flex justify-center items-center bg-amber-400 h-20 w-4/5 mb-5">
            <h1 class=" text-black text-5xl text-center">En Construccion</h1>
        </div>            
    </div>
    <div class="flex justify-center items-center">
        <Image src={cuerpo} alt="cuerpo" width="300" height="300" />
    </div>
</Layout>